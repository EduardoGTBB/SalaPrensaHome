    <script>
    GetTagSource();
    var $_GET = $.requestParams();
    if ($_GET['file'] != null) {

        var urlSrc = "resources/laJudicaturaHoy/2023/audios/";
        if ($_GET['file'].indexOf("mp3") >= 0) {

            $("#ul-resources-boletin li:first-child span.title").text("Escúchanos");
            $("ul #li-Audios-Menu").addClass("active");
            $("ol.breadcrumb li.active").text("Audios");

            var audio = $("#audioplayer");
            audio.find("#mp3_src").attr("src", urlSrc + $_GET['file']);
            /****************/
            audio[0].pause();
            audio[0].load(); //suspends and restores all audio element

            //audio[0].play(); //changed based on Sprachprofi's comment below
            audio[0].oncanplaythrough = audio[0].play();
            /****************/

            if ($_GET['bIsJudicaHoy'] != null) {
                $("#ul-resources-boletin li:nth-child(2)").show();
            }

            if ($_GET['genericFileTitle'] != null) {
                if ($_GET['genericFileTitle'].toLowerCase().indexOf("recordar") >= 0)
                    $("h2.title-divider").html($("h2.title-divider").html() + ", <span style='background-color:#ebecf1'>lo que hay que recordar</span>")
                $("#text-subtitle").text($_GET['genericFileTitle']);
            }

        }
        /*else

        {

        }*/

    }
    </script>
